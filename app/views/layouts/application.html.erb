<!DOCTYPE html>
<html>
  <head>
    <title>NoteManager</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |name, msg|%>
    <h3><%= msg %></h3>
    <% end %>
    <%= yield %>
  </body>
</html>
