 <h1 align="center">Edit Note</h1>

<div align="center">
<%= form_with url: "/users/#{current_user.id}/note/#{@note.id}", model: @note, method: :patch, local: true do |f|%>
    <%= f.label :title, "Title: " %><br>
    <%= f.text_field :title %><br><br>

    <%= f.label :body, "Body Text: " %><br>
    <%= f.text_area :body, size: "70x5" %><br>

    <%= f.submit "Update Note" %>
<% end %> 
</div>